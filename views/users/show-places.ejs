<html>
  <head>
    <meta charset="utf-8"
    <title></title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  </head>
  <body>
    <div class="container">
    <center>
      <h3>It'a good choice to pick <%=data_genre.name%> music as favorite Genre</h3>
      <h3>We suggest u to go to these Places</h3>
    </center>
    <br>
    <br>
      <table class="table table-bordered">
        <thead>
          <tr>
            <th>No</th>
            <th>Name of Club</th>
            <th>Address</th>
            <th>Action</th>
          </thead>
          <tbody>
            <%data_genre.Places.forEach((data,i) =>{%>
              <tr>
                <td><%=i+1%></td>
                <td>
                  <%=data.name%>
                </td>
                <td><%=data.address%></td>
                <td>
                  <a href="/users/<%=data_user.id%>/viewreview/<%=data.id%>" class="btn btn-info">View Review</a>
                  <a href="/users/<%=data_user.id%>/addreview/<%=data.id%>" class="btn btn-success">Add Review</a>
                </td>
              </tr>
            <%})%>
          </tbody>
    </div>
    </table>
  </body>
</html>
